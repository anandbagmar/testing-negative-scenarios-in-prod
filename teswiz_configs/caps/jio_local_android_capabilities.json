{
  "android": {
    "platformName": "android",
    "automationName": "UiAutomator2",
    "autoGrantPermissions": true,
    "appPackage": "io.specmatic.e2edemo",
    "appiumVersion": "3.1.2",
    "uiautomator2ServerInstallTimeout": 10000,
    "adbExecTimeout": 45000,
    "eventTimings": true,
    "enablePerformanceLogging": true,
    "printPageSourceOnFindFailure": true,
    "app": "sampleApps/MockedE2EDemo-debug.apk",
    "deviceName": "android",
    "noReset": false,
    "fullReset": false,
    "appium:clearDeviceLogsOnStart": true,
    "noSign": true
  },
  "serverConfig": {
    "server": {
      "use-plugins": [
        "device-farm"
      ],
      "plugin": {
        "device-farm": {
          "platform": "android",
          "skipChromeDownload": true
        }
      }
    }
  },
  "appiumServerPath": "./node_modules/appium/build/lib/main.js"
}
